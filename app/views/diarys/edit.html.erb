<h1>ダイアリー編集</h1>
<%= form_with model:@diary, local:true do |f| %>
  <h4>画像</h4>
  <% @diary.diary_images.each do |image| %>
    <%= attachment_image_tag image, :image, class: "default_image" %>
  <% end %>
  <%= f.attachment_field :diary_images_images, multiple: true %>
  <h4>題名</h4>
  <%= f.text_field :title %>
  <h4>説明</h4>
  <%= f.text_area :body %>
  <%= f.submit '編集' %>
<% end %>
